{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ui": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfill.ts",
            "assets": [
              "src/asset",
              "src/favicon.ico",
              "src/json",
              "src/web.config",
              "src/unsupported.html"
            ],
            "styles": [
              "node_modules/normalize.css/normalize.css",
              "node_modules/dragula/dist/dragula.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/asset/scss/styles.scss"
            ],
            "scripts": [
              "node_modules/dom-autoscroller/dist/dom-autoscroller.js",
              "polyfills/closest.js"
            ],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ui:build"
          },
          "configurations": {
            "prod": {
              "browserTarget": "ui:build:prod"
            },
            "production": {
              "browserTarget": "ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfill.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "codeCoverageExclude": [
              "**/test.ts",
              "**/polyfill.ts",
              "**/mocks/**/*"
            ],
            "scripts": [
              "node_modules/dom-autoscroller/dist/dom-autoscroller.js",
              "polyfills/closest.js"
            ],
            "styles": [
              "node_modules/normalize.css/normalize.css",
              "node_modules/dragula/dist/dragula.css",
              "src/asset/scss/styles.scss"
            ],
            "assets": [
              "src/asset",
              "src/favicon.ico",
              "src/json",
              "src/web.config",
              "src/unsupported.html"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    },
    "ui-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "ui:serve"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "analytics": "93bb7fb9-075d-4507-9bee-f63aaf91c18b",
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
